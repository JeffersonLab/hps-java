        <table key="ecal_bad_channels" name="ecal_bad_channels">
            <classes>
                <object class="org.hps.conditions.ecal.EcalBadChannel"/>
                <collection class="org.hps.conditions.ecal.EcalBadChannel$EcalBadChannelCollection"/>
            </classes>
