                <collection class="org.hps.conditions.ecal.EcalChannel$EcalChannelCollection"/>
            </classes>
            <fields>
                <field name="channel_id" />                
                <field name="crate" />
                <field name="slot" />
                <field name="channel" />
                <field name="x" />
                <field name="y" />
